<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/serie_details_navigation"
    app:startDestination="@id/navigation_serie_details_master">

    <fragment
        android:id="@+id/navigation_serie_details_master"
        android:name="revolhope.splanes.com.aikver.presentation.feature.menu.common.content.SerieDetailsMasterFragment"
        tools:layout="@layout/fragment_serie_details_master">
        <action
            android:id="@+id/action_serie_details_master_to_slave"
            app:destination="@id/navigation_serie_details_slave" />
    </fragment>

    <fragment
        android:id="@+id/navigation_serie_details_slave"
        android:name="revolhope.splanes.com.aikver.presentation.feature.menu.common.content.SerieDetailsSlaveFragment"
        tools:layout="@layout/fragment_serie_details_slave">
        <action
            android:id="@+id/action_serie_details_slave_to_master"
            app:destination="@id/navigation_serie_details_master" />
    </fragment>

</navigation>